<?xml version="1.0" encoding="UTF-8" standalone="no"?><arguments>
    <programname>VolumeCalculator</programname>
    <programdescription>Calculate the volume of a cube, cylinder, or sphere.
Order: 
  Box: Length, Width, Height
  Cylinder: Height Radius (IGNORED)
  Sphere: Radius (IGNORED) (IGNORED)</programdescription>
<positional>
    <position>1</position>
    <name>firstNum</name>
    <type>float</type>
    <description>the length of the object (float)</description>
</positional>
<positional>
    <position>2</position>
    <name>secondNum</name>
    <type>float</type>
    <description>the width of the object (float)</description>
</positional>
<positional>
    <position>3</position>
    <name>thirdNum</name>
    <type>float</type>
    <description>the height of the object (float)</description>
</positional>
<named>
    <name>type</name>
    <type>string</type>
    <restrictedvalues>box, cylinder, sphere</restrictedvalues>
    <description>Shape of object: box, cylinder, or sphere.</description>
    <shortname>t</shortname>
    <required>true</required>    <default>box</default>
</named>
<named>
    <name>digits</name>
    <type>integer</type>
    <description>decimal precision for answer.</description>
    <shortname>d</shortname>
    <default>2</default>
</named>
</arguments>